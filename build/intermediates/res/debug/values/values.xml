    </style>
    <style name="custom_dialog" parent="@android:style/Theme.Dialog">
        <item name="android:windowFrame">@null</item>
        <!-- whether has border -->
        <item name="android:windowIsFloating">true</item>
        <!-- whether suspend on activity -->
        <item name="android:windowIsTranslucent">true</item>
        <!-- whether translucent -->
        <item name="android:windowNoTitle">true</item>
        <!-- set whether has title -->
        <item name="android:windowBackground">@android:color/transparent</item>
        <!-- set bg transparent -->
        <item name="android:backgroundDimEnabled">false</item>
        <!-- set bg fuzzy display -->
        <item name="android:backgroundDimAmount">0.6</item>
        <!-- gray -->
        <item name="android:windowContentOverlay">@null</item>
        <!-- whether the dialog box with cover -->
        <item name="android:windowAnimationStyle">@style/dialog_animation</item>
        <!-- the animation  when entering or popping up -->
        <item name="android:colorBackgroundCacheHint">@null</item>
        <!-- background color of the cache -->
        <item name="android:background">@android:color/transparent</item>
        <!-- os transparent -->
    </style>
    <style name="custom_switch">
        <item name="android:track">@drawable/switch_bg</item>
        <item name="android:thumb">@drawable/image_style_thumb</item>
        <item name="android:switchTextAppearance">@android:style/TextAppearance.Large</item>
